@using SuperSold.UI.AspDotNet.Extensions;
@using SuperSold.UI.AspDotNet.ViewRouting;
@model IEnumerable<AccountInfoModel>

<h1>Admin Area</h1>
<span>for accounts management</span>

<div>

    @foreach(var account in Model) {

        <div class="item-row" style="border:solid; border-width:2px; border-radius:5px;">
            @await Component.AccountDisplay().DisplayAsRow(account)
        </div>

    }

</div>

